{
	"name": "fate-core",
	"private": true,
	"version": "1.0.2",
	"repository": {
		"url": "https://github.com/Stanislavsonder/fate-core.git"
	},
	"author": {
		"name": "Stanislav Sonder",
		"email": "stanislavsonder@gmail.com"
	},
	"type": "module",
	"lint-staged": {
		"*.{js,ts,vue,json}": [
			"eslint .",
			"prettier --write",
			"vitest --run"
		]
	},
	"scripts": {
		"dev": "vite --host",
		"translate": "node --loader ts-node/esm ./src/scripts/localizer/index.ts && prettier --write ./src/i18n/locales",
		"build:android": "npm run build && npx cap sync android && npx cap open android",
		"build:ios": "npm run build && npx cap sync ios && npx cap open ios",
		"build": "vue-tsc && vite build",
		"preview": "vite preview",
		"test:e2e": "cypress run",
		"test:unit": "vitest",
		"lint": "eslint .",
		"format": "prettier --write .",
		"test": "npm run test:unit && npm run test:e2e",
		"prepare": "husky"
	},
	"dependencies": {
		"@capacitor/android": "^7.0.1",
		"@capacitor/app": "7.0.0",
		"@capacitor/core": "7.0.1",
		"@capacitor/dialog": "^7.0.0",
		"@capacitor/haptics": "^7.0.0",
		"@capacitor/ios": "^7.0.1",
		"@capacitor/keyboard": "^7.0.0",
		"@capacitor/motion": "^7.0.0",
		"@capacitor/status-bar": "^7.0.0",
		"@ionic/pwa-elements": "^3.3.0",
		"@ionic/vue": "^8.4.2",
		"@ionic/vue-router": "^8.4.2",
		"@vueuse/core": "^12.5.0",
		"cannon-es": "^0.20.0",
		"dexie": "^4.0.11",
		"ionicons": "^7.4.0",
		"markdown-it": "^14.1.0",
		"pinia": "^2.3.1",
		"three": "^0.172.0",
		"ts-node": "^10.9.2",
		"vue": "^3.5.13",
		"vue-i18n": "^11.0.1",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@capacitor/cli": "7.0.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@commitlint/format": "^19.5.0",
		"@commitlint/types": "^19.5.0",
		"@eslint/js": "^9.19.0",
		"@tailwindcss/postcss": "4.0.0",
		"@tailwindcss/vite": "4.0.0",
		"@types/cannon": "^0.1.13",
		"@types/markdown-it": "^14.1.2",
		"@types/three": "^0.172.0",
		"@typescript-eslint/eslint-plugin": "^8.22.0",
		"@typescript-eslint/parser": "^8.22.0",
		"@vitejs/plugin-legacy": "^6.0.0",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue/eslint-config-typescript": "^14.3.0",
		"@vue/test-utils": "^2.4.6",
		"commitlint": "^19.6.1",
		"conventional-changelog-atom": "^5.0.0",
		"cypress": "^14.0.1",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-vue": "^9.32.0",
		"globals": "^15.14.0",
		"husky": "^9.1.7",
		"jsdom": "^26.0.0",
		"lint-staged": "^15.4.3",
		"prettier": "^3.4.2",
		"prettier-plugin-sort-json": "^4.1.1",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"tailwindcss": "^4.0.0",
		"terser": "^5.37.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.22.0",
		"vite": "^6.0.11",
		"vite-plugin-pwa": "^0.21.1",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.0.4",
		"vue-tsc": "^2.2.0"
	},
	"description": "FATE: Core - A digital character sheet for the FATE Core RPG system."
}
